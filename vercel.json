{
  "buildCommand": "bash build.sh",
  "outputDirectory": "dist/osm/browser",
  "installCommand": "npm ci",
  "env": {
    "GH_USER": "",
    "GH_REPO": "",
    "GH_PATH": "/v2/",
    "GH_BRANCH": "main"
  },
  "redirects": [
    {
      "source": "/",
      "destination": "/zh-Hans/",
      "has": [
        {
          "type": "header",
          "key": "Accept-Language",
          "value": "zh"
        }
      ],
      "permanent": true
    },
    {
      "source": "/",
      "destination": "/en-US/",
      "missing": [
        {
          "type": "header",
          "key": "Accept-Language",
          "value": "zh"
        }
      ],
      "permanent": true
    }
  ]
}